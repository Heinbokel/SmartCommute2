@model SmartCommuteEmmet.Models.ConfigDate

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>ConfigDate</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Create" class="genericForm">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StartDate" class="control-label"></label>
                <input asp-for="StartDate" placeholder="When does smart commute week start?" class="form-control" />
                <span asp-validation-for="StartDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EndDate" class="control-label"></label>
                <input asp-for="EndDate" placeholder="When does smart commute end for the Summer?" class="form-control" />
                <span asp-validation-for="EndDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RegisterByDate" class="control-label"></label>
                <input asp-for="RegisterByDate" placeholder="When should teams register by?" class="form-control" />
                <span asp-validation-for="RegisterByDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn-main" />
            </div>
        </form>
    </div>
</div>

