
@{
    ViewData["Title"] = "ManagementConsole";
}
<div class="genericSection">
    <h1 class="aboutHeader">Management Console</h1>
    <div class="aboutParagraph">
        <h2>Configuration:</h2>
        <input type="button" class="btn-main" value="Sponsors" onclick="location.href='@Url.Action("Index", "Sponsors")'" />
        <input type="button" class="btn-main" value="Breakfasts" onclick="location.href='@Url.Action("Index", "Breakfasts")'" />
        <input type="button" class="btn-main" value="Date Configs" onclick="location.href='@Url.Action("Index", "ConfigDates")'" />
        <input type="button" class="btn-main" value="Sponsor Carousel" onclick="location.href='@Url.Action("Index", "CarouselSponsorImages")'" />
        <input type="button" class="btn-main" value="Documents" onclick="location.href='@Url.Action("Index", "Documents")'" />
        <input type="button" class="btn-main" value="Businesses" onclick="location.href='@Url.Action("Index", "Businesses")'" />
        <input type="button" class="btn-main" value="Rewards" onclick="location.href='@Url.Action("Index", "Rewards")'" />
        <hr />
        <h2>Commute Select List Data:</h2>
        <input type="button" class="btn-main" value="Commute Types" onclick="location.href='@Url.Action("Index", "CommuteTypes")'" />
        <input type="button" class="btn-main" value="Start Points" onclick="location.href='@Url.Action("Index", "StartPoints")'" />
        <input type="button" class="btn-main" value="End Points" onclick="location.href='@Url.Action("Index", "EndPoints")'" />
        <hr />
        <h2>Reports and Tools:</h2>
        <input type="button" class="btn-main" value="Users" onclick="location.href='@Url.Action("ManageUsers", "Manage")'" />
        <input type="button" class="btn-main" value="All Commutes" onclick="location.href='@Url.Action("ManageCommutes", "Manage")'" />
        <input type="button" class="btn-main" value="Business Commutes" onclick="location.href='@Url.Action("ManageCommutesBusiness", "Manage")'" />
        <input type="button" class="btn-main" value="Email" onclick="location.href='@Url.Action("Index", "Rewards")'" />
    </div>
</div>
